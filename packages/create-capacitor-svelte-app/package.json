{
	"name": "create-capacitor-svelte-app",
	"version": "0.0.25",
	"description": "Use this CLI app to setup a new Capacitor application in a new SvelteKit project.",
	"keywords": [
		"capacitor",
		"ionic",
		"svelte",
		"sveltekit",
		"svelte-kit"
	],
	"homepage": "https://ionic-svelte.firebaseapp.com",
	"repository": {
		"type": "git",
		"url": "https://github.com/Tommertom/svelte-ionic-npm",
		"directory": "packages/create-capacitor-svelte-app/"
	},
	"license": "MIT",
	"author": "Tommertom",
	"type": "module",
	"main": "./src/index.js",
	"bin": {
		"create-ionic-svelte-app": "./src/index.js"
	},
	"files": [
		"src/**"
	],
	"engines": {
		"node": ">=14.16"
	},
	"scripts": {
		"dev": "node src/index.js",
		"long": "node src/index.js --types=typescript --prettier --eslint --playwright=false -f svelte-kit --ionicons=false",
		"pub:beta": "npm publish --tag beta",
		"pub:next": "npm publish --tag next",
		"pub:release": "npm publish",
		"release": "npm publish",
		"start": "node dist/index.js"
	},
	"dependencies": {
		"create-svelte": "^6.3.5",
		"fs-extra": "^10.1.0",
		"ip": "^1.1.8",
		"kleur": "^4.1.5",
		"mri": "^1.2.0",
		"prompts": "^2.4.2"
	},
	"devDependencies": {
		"bumpp": "^8.2.1",
		"eslint": "^8.28.0",
		"fs": "^0.0.1-security",
		"path": "^0.12.7",
		"standard-version": "^9.5.0",
		"svelte": "^4.2.7"
	}
}